use crate::prelude::*;

/// Core data structures
#[derive(Debug, Clone)]
pub struct Document {
    pub title: String,
    pub content: String,
    pub metadata: DocumentMetadata,
}

impl Document {
    pub fn new(title: String, content: String) -> Self {
        Self {
            title,
            content,
            metadata: DocumentMetadata::default(),
        }
    }
}
